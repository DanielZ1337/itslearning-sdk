{
  "name": "itslearning-sdk",
  "version": "1.0.0",
  "description": "A lightweight SDK for itslearning's REST API.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./types": {
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "itslearning",
    "sdk",
    "typescript",
    "fetch",
    "bun"
  ],
  "license": "MIT",
  "author": "Daniel Bermann Schmidt <danielz2nd@hotmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/DanielZ1337/itslearning-sdk.git"
  },
  "scripts": {
    "build": "npm run generate-barrels && tsup",
    "generate-barrels": "barrelsby -c barrelsby.json",
    "test": "bun test",
    "biome:format": "biome format --write ./src",
    "biome:lint": "biome lint --write ./src",
    "biome:check": "biome check --write ./src",
    "biome:check:fix": "biome check --fix ./src",
    "biome:check:fix-unsafe": "biome check --fix --unsafe ./src"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/bun": "latest",
    "barrelsby": "^2.8.1",
    "tsup": "^8.3.5"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "engines": {
    "bun": ">=1.0.0",
    "node": ">=16.0.0"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "esbuild"
  ],
  "dependencies": {
    "axios": "^1.7.9"
  }
}
